Num recfib(Num n) {
    Num iszero
    iszero = ~n
    Num isone
    isone = n - 1
    isone = ~isone
    isone ?? {
        result = 1
    }
    ~isone ?? {
        ~iszero ?? {
            Num a
            a = n - 2
            Num b
            b = n - 1
            result = recfib(a) + recfib(b)
        }
    }
}

Num main() {
    Num n
    n = 6
    result = recfib(n)
    result = result + 48
}
